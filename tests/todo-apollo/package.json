{
  "name": "@gourmet/test-todo-apollo",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "build": "gourmet build",
    "start": "node lib/server.js",
    "dev": "nodemon --ignore src lib/server.js -- --watch",
    "test": "npm run test:local && npm run test:prod && npm run test:ltc",
    "test:local": "gourmet build --verbosity 1 && tape test/*_test.js --verbosity 1",
    "test:prod": "gourmet build --verbosity 1 --stage prod && tape test/*_test.js --stage prod --verbosity 1",
    "test:ltc": "gourmet build --verbosity 1 --stage ltc && tape test/*_test.js --stage ltc --verbosity 1"
  },
  "dependencies": {
    "@gourmet/client-lib": "^1.2.1",
    "@gourmet/console": "^1.1.1",
    "@gourmet/server-args": "^1.2.2",
    "apollo-datasource": "^0.3.1",
    "apollo-server-express": "^2.4.8",
    "express": "^4.16.3",
    "graphql": "^14.1.1",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "@gourmet/gourmet-cli": "^1.1.1",
    "@gourmet/group-react-apollo": "^1.0.1",
    "@gourmet/preset-react": "^1.2.6",
    "@gourmet/promise-tape": "^0.4.1",
    "core-js": "2",
    "graphql-tag": "^2.10.1",
    "nodemon": "^1.18.10",
    "puppeteer": "^1.3.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "tape": "^4.9.0"
  }
}
